<?xml version="1.0" encoding="UTF-8"?>
<ld:linux-distributions
    xmlns:ld="wrns"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="wrns linux-distibutions.xsd"
    url="https://www.pngall.com/wp-content/uploads/5/Linux-PNG-Pic.png">
    <ld:linux-distribution
        name="Mandriva"
        active="false"
        icon-url="https://commons.wikimedia.org/wiki/File:Mandriva_logo.png"
        latest-stable-release="2011">
        <ld:package-type>rpm</ld:package-type>
        <ld:versioning-model>Standard</ld:versioning-model>
        <ld:first-release>1998-07-23</ld:first-release>
        <ld:website-url>https://web.archive.org/web/20150523021945/http://www.mandriva.com/en/products-services/mbs/technical-specifications/</ld:website-url>
    </ld:linux-distribution>
    <ld:linux-distribution
        name="Fedora"
        icon-url="https://commons.wikimedia.org/wiki/File:Fedora_logo.svg"
        latest-stable-release="37">
        <ld:package-type>rpm</ld:package-type>
        <ld:versioning-model>Standard</ld:versioning-model>
        <ld:first-release>2003-11-06</ld:first-release>
        <ld:website-url>https://getfedora.org</ld:website-url>
    </ld:linux-distribution>
    <ld:linux-distribution
        name="Debian"
        icon-url="https://commons.wikimedia.org/wiki/File:Debian_logo.png"
        latest-stable-release="11.2">
        <ld:package-type>deb</ld:package-type>
        <ld:versioning-model>Standard</ld:versioning-model>
        <ld:first-release>1993-09-15</ld:first-release>
        <ld:website-url>https://www.debian.org</ld:website-url>
    </ld:linux-distribution>
    <ld:linux-distribution
        name="Red Hat Enterprise Linux (RHEL)"
        icon-url="https://de.m.wikipedia.org/wiki/Datei:Red_Hat_logo.svg"
        latest-stable-release="9.1">
        <ld:package-type>rpm</ld:package-type>
        <ld:versioning-model>Standard</ld:versioning-model>
        <ld:first-release>2000-02-22</ld:first-release>
        <ld:website-url>https://www.redhat.com/rhel/</ld:website-url>
    </ld:linux-distribution>
    <ld:linux-distribution
        name="Gentoo"
        icon-url="https://commons.wikimedia.org/wiki/File:Gentoo_Logo_Vector.svg">
        <ld:package-type>ebuild</ld:package-type>
        <ld:versioning-model>Rolling</ld:versioning-model>
        <ld:first-release>2002-03-31</ld:first-release>
        <ld:website-url>https://www.gentoo.org</ld:website-url>
    </ld:linux-distribution>
    <ld:linux-distribution
        name="CentOS"
        icon-url="https://commons.wikimedia.org/wiki/File:CentOS_logo.svg">
        <ld:package-type>rpm</ld:package-type>
        <ld:versioning-model>Rolling</ld:versioning-model>
        <ld:first-release>2004-05-14</ld:first-release>
        <ld:website-url>https://www.centos.org</ld:website-url>
    </ld:linux-distribution>
    <ld:linux-distribution
        name="Ubuntu"
        icon-url="https://upload.wikimedia.org/wikipedia/commons/thumb/1/16/Ubuntu_and_Ubuntu_Server_Icon.png/48px-Ubuntu_and_Ubuntu_Server_Icon.png"
        latest-stable-release="22.10 (Kinetic Kudu)">
        <ld:package-type>deb</ld:package-type>
        <ld:versioning-model>Standard</ld:versioning-model>
        <ld:first-release>2004-10-20</ld:first-release>
        <ld:website-url>https://www.ubuntu.com</ld:website-url>
    </ld:linux-distribution>
    <ld:linux-distribution
        name="OpenSUSE Tumbelweed"
        icon-url="https://upload.wikimedia.org/wikipedia/commons/thumb/4/49/OpenSUSE_Tumbleweed_green_logo.svg/160px-OpenSUSE_Tumbleweed_green_logo.svg.png">
        <ld:package-type>rpm</ld:package-type>
        <ld:versioning-model>Rolling</ld:versioning-model>
        <ld:first-release>2005-10-01</ld:first-release>
        <ld:website-url>https://www.opensuse.org</ld:website-url>
    </ld:linux-distribution>
</ld:linux-distributions>
